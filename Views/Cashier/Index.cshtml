@model List<Cashier>
<div>
    <a href="/Cashier/Add" class="btn btn-primary">Create Casheir</a>
</div>
<figure>Casheir Data</figure>
<table class="table table-hover table-bordered">
    <tr  class="text-center bg-dark" style="color:white;font-weight:600">
           <td>ID</td>
           <td>Branch</td>
           <td>Casheir</td>
           <td>Details</td>
       </tr>
    @foreach (var item in Model)
   {
         
       <tr class="text-center">
           <td>@item.Id</td>
           <td>@item.Branch.BranchName</td>
           <td>@item.CashierName</td>
           <td><a href="/Cashier/GetOne/@item.Id" class="btn btn-primary">Detail</a></td>
       </tr>
   }
</table>